    "prettier-plugin-import-sort": "0.0.7",
    "typescript": "^5.1.6",
    "vite": "^4.4.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md,json}": "eslint --no-ignore --cache --fix --max-warnings=0"
